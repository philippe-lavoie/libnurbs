####### Compiler, tools and options

CC       = gcc
CXX      = g++
LEX      = flex
YACC     = yacc
CFLAGS   = -pipe -Wall -W -O2 -DQT_NO_DEBUG
CXXFLAGS = -pipe -Wall -W -O2 -DQT_NO_DEBUG
LEXFLAGS = 
YACCFLAGS= -d
INCPATH  = -I$(QTDIR)/include -I.ui/ -I../nurby3 -I.moc/ -I/usr/local/qt/mkspecs/default
LINK     = g++
LFLAGS   = --whole-archive
LIBS     = $(SUBLIBS) -Wl,-rpath,$(QTDIR)/lib -L$(QTDIR)/lib -L/usr/X11R6/lib -lqt -lXext -lX11 -lm
AR       = ar cqs
RANLIB   = 
MOC      = $(QTDIR)/bin/moc
UIC      = $(QTDIR)/bin/uic
QMAKE    = qmake
TAR      = tar -cf
GZIP     = gzip -9f
COPY     = cp -f
COPY_FILE= $(COPY) -p
COPY_DIR = $(COPY) -pR
DEL      = rm -f
MOVE     = mv

####### Output directory

OBJECTS_DIR = .obj/

####### Files

HEADERS = nurby/GLDisplay.h
SOURCES = nurby/GLDisplay.cpp
OBJECTS = .obj/GLDisplay.o \
		.obj/form1.o \
		.obj/qmake_image_collection.o
FORMS = form1.ui
UICDECLS = .ui/form1.h
UICIMPLS = .ui/form1.cpp
SRCMOC   = .moc/moc_GLDisplay.cpp \
		.moc/moc_form1.cpp
OBJMOC = .obj/moc_GLDisplay.o \
		.obj/moc_form1.o
DIST	   = 
QMAKE_TARGET = nurby
DESTDIR  = 
TARGET   = nurby

first: all
####### Implicit rules

.SUFFIXES: .cpp .cxx .cc .C .c

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules

all: Makefile $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: nurby.pro  
	$(QMAKE) nurby.pro
qmake: 
	@$(QMAKE) nurby.pro

dist: 
	@mkdir -p .obj/nurby && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/nurby/ && ( cd `dirname .obj/nurby` && $(TAR) nurby.tar nurby && $(GZIP) nurby.tar ) && mv `dirname .obj/nurby`/nurby.tar.gz . && rm -rf .obj/nurby

mocclean:
	-rm -f $(OBJMOC)
	-rm -f $(SRCMOC)

uiclean:
	-rm -f $(UICIMPLS) $(UICDECLS)

clean: mocclean uiclean
	-rm -f $(OBJECTS) 
		-rm -f .ui/qmake_image_collection.cpp
	-rm -f *~ core *.core


####### Sub-libraries

distclean: clean
	-rm -f $(TARGET) $(TARGET)
	

####### Compile

.obj/GLDisplay.o: nurby/GLDisplay.cpp nurby/GLDisplay.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/GLDisplay.o nurby/GLDisplay.cpp

.ui/form1.h: form1.ui form1.ui.h
	$(UIC) form1.ui -o .ui/form1.h

.ui/form1.cpp: .ui/form1.h form1.ui form1.ui.h
	$(UIC) form1.ui -i form1.h -o .ui/form1.cpp

.obj/form1.o: .ui/form1.cpp .ui/form1.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/form1.o .ui/form1.cpp

.obj/moc_GLDisplay.o: .moc/moc_GLDisplay.cpp nurby/GLDisplay.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_GLDisplay.o .moc/moc_GLDisplay.cpp

.obj/moc_form1.o: .moc/moc_form1.cpp .ui/form1.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_form1.o .moc/moc_form1.cpp

.moc/moc_GLDisplay.cpp: $(MOC) nurby/GLDisplay.h
	$(MOC) nurby/GLDisplay.h -o .moc/moc_GLDisplay.cpp

.moc/moc_form1.cpp: $(MOC) .ui/form1.h
	$(MOC) .ui/form1.h -o .moc/moc_form1.cpp

.obj/qmake_image_collection.o: .ui/qmake_image_collection.cpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/qmake_image_collection.o .ui/qmake_image_collection.cpp

.ui/qmake_image_collection.cpp: images/filenew \
		images/fileopen \
		images/filesave \
		images/print \
		images/undo \
		images/redo \
		images/editcut \
		images/editcopy \
		images/editpaste \
		images/searchfind
	$(UIC)  -embed nurby images/filenew images/fileopen images/filesave images/print images/undo images/redo images/editcut images/editcopy images/editpaste images/searchfind -o .ui/qmake_image_collection.cpp

####### Install

install: all 

